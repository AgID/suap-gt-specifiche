<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
       
    xmlns:stresvi="http://agid.it/suap/sections/trasferimento_esercizio_vicinato"
    
    xmlns:ecencom="http://agid.it/suap/entities/centro_commerciale"
    xmlns:esetmer="http://agid.it/suap/entities/settori_merceologici"
    xmlns:eindita="http://agid.it/suap/entities/indirizzo_italiano"
    xmlns:econt="http://agid.it/suap/entities/contatti"
        
    targetNamespace="http://agid.it/suap/sections/trasferimento_esercizio_vicinato"
    elementFormDefault="qualified" attributeFormDefault="qualified"    
    version="1.0.0" xml:lang="it">

    <xsd:import namespace="http://agid.it/suap/entities/centro_commerciale" schemaLocation="../02_entities/ent_centro_commerciale_v1.0.0.xsd"/>
    <xsd:import namespace="http://agid.it/suap/entities/settori_merceologici" schemaLocation="../02_entities/ent_settori_merceologici_v1.0.0.xsd"/>
    <xsd:import namespace="http://agid.it/suap/entities/indirizzo_italiano" schemaLocation="../02_entities/ent_indirizzo_italiano_v1.0.0.xsd"/>
    <xsd:import namespace="http://agid.it/suap/entities/contatti" schemaLocation="../02_entities/ent_contatti_v1.0.0.xsd"/>
    
    <xsd:element name="trasferimento_esercizio_vicinato" type="stresvi:trasferimento_esercizio_vicinato_type"/>
    
    <xsd:complexType name="trasferimento_esercizio_vicinato_type">
        <xsd:sequence>
            <xsd:element name="da">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="indirizzo" type="eindita:indirizzo_italiano_ctype" minOccurs="1" maxOccurs="1"/>
                        <xsd:element name="in_centro_commerciale" type="ecencom:centro_commerciale_ctype" minOccurs="0" maxOccurs="1"/>        
                    </xsd:sequence>
                </xsd:complexType>                
            </xsd:element>            
            <xsd:element name="a">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="indirizzo" type="eindita:indirizzo_italiano_ctype" minOccurs="1" maxOccurs="1"/>
                        <xsd:element name="contatti" type="econt:contatti_ctype" minOccurs="1" maxOccurs="1"/>
                        <xsd:element name="in_centro_commerciale" type="ecencom:centro_commerciale_ctype" minOccurs="0" maxOccurs="1"/>        
                    </xsd:sequence>
                </xsd:complexType>    
            </xsd:element>           
            <xsd:element name="settori_merceologici" type="esetmer:settori_merceologici_ctype" minOccurs="1" maxOccurs="1"/>            
        </xsd:sequence>        
    </xsd:complexType>    
</xsd:schema>
