<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:vqual="http://vocabolari/qualifica"
    
    xmlns:equal="http://entita/qualifica"
    targetNamespace="http://entita/qualifica"
    elementFormDefault="qualified" attributeFormDefault="qualified"    
    version="1.0.0" xml:lang="it">
    
    <xs:import namespace="http://vocabolari/qualifica" schemaLocation="../01_vocabolari/voc_qualifica_v1.0.0.xsd"/>
    
    <xs:simpleType name="qualifica_stype">
        <xs:restriction base="vqual:qualifica_vocabulary">
    </xs:simpleType>
    
    <xs:complexType name="qualifica_ctype">
        <xs:sequence>
            <xs:element name="qualifica" type="equal:qualifica_stype" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>        
    </xs:complexType>
    
</xs:schema>
