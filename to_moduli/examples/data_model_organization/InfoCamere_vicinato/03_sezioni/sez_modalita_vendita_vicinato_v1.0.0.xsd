            <xs:extension base="smvv:modalita_vendita_vicinato_atype">
                 <xs:choice>
                        <xs:element name="vendita_dettaglio" type="xs:decimal">
                        </xs:element>
                        <xs:element name="vendita_congiunta_dettaglio_ingrosso" type="xs:decimal">
