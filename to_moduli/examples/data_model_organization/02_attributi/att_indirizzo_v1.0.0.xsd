    </xs:simpleType>    
    
    <xs:simpleType name="numero_civico_stype">
        <xs:restriction base="xs:integer">
            <xs:minExclusive value="0"/>
        </xs:restriction>
    </xs:simpleType>
    
</xs:schema>
