    xmlns:smvv="http://sezione/modalita_vendita_vicinato"
    xmlns:sse="http://sezione/stagionalita_esercizio"
    xmlns:stav="http://sezione/tipologia_attivita_vicinato"
      
    xmlns:mvicinato="http://moduli/apertura_vicinato"
    targetNamespace="http://moduli/apertura_vicinato"
    elementFormDefault="qualified" attributeFormDefault="qualified"    
    version="1.0.0" xml:lang="it">
    
  <xs:import namespace="http://sezione/dati_catastali" schemaLocation="../03_sezioni/sez_dati_catastali_v1.0.0.xsd"/>
  <xs:import namespace="http://sezione/in_centro_commerciale_vicinato" schemaLocation="../03_sezioni/in_centro_commerciale_vicinato_v1.0.0.xsd"/>
  <xs:import namespace="http://sezione/impresa" schemaLocation="../03_sezioni/sez_impresa_v1.0.0.xsd"/>
