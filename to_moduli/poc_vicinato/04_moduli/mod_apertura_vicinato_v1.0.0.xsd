<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" 
    xmlns:sdc="http://sezione/dati_catastali"
    xmlns:sicm="http://sezione/in_centro_commerciale_vicinato"
