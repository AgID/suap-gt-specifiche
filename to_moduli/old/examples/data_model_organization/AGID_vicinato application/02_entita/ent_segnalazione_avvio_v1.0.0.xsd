    targetNamespace="http://entita/segnalazione_avvio"
    elementFormDefault="qualified" attributeFormDefault="qualified"    
    version="1.0.0" xml:lang="it">
    
    <xs:simpleType name="esercizio_vicinato_stype">
        <xs:restriction base="xs:boolean"/>
    </xs:simpleType>
    
    <xs:simpleType name="esclusiva_apparecchi_automatici_stype">
