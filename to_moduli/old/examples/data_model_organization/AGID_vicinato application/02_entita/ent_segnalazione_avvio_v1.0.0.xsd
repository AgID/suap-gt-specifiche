    </xs:simpleType>
    
    <xs:complexType name="segnalazione_avvio_ctype">
        <xs:sequence>
            <xs:element name="esercizio_vicinato" type="eseav:esercizio_vicinato_stype" minOccurs="0" maxOccurs="1"/>
            <xs:element name="esclusiva_apparecchi_automatici" type="eseav:esclusiva_apparecchi_automatici_stype" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
</xs:schema>
