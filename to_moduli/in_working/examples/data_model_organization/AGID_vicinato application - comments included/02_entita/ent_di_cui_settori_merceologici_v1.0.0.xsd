<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" 
    xmlns:earve="http://entita/area_vendita"
    
    xmlns:edcseme="http://entita/di_cui_settori_merceologici"
    targetNamespace="http://entita/di_cui_settori_merceologici"
    elementFormDefault="qualified" attributeFormDefault="qualified"    
    version="1.0.0" xml:lang="it">
    
    <xs:import namespace="http://entita/area_vendita" schemaLocation="ent_area_vendita_v1.0.0.xsd"/>
    
    <xs:complexType name="di_cui_settori_merceologici_ctype" abstract="true">
        <xs:sequence> 
            <xs:element name="prodotti_tabelle_speciali" type="earve:prodotti_tabelle_speciali_ctype" minOccurs="0" maxOccurs="unbounded"/>            
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="di_cui_settori_merceologici_ctype_italia">
        <xs:complexContent>
            <xs:extension base="edcseme:di_cui_settori_merceologici_ctype">
                <xs:sequence>
                    <xs:element name="merci_ingombranti" type="earve:merci_ingombranti_ctype" minOccurs="0" maxOccurs="unbounded"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    
</xs:schema>
