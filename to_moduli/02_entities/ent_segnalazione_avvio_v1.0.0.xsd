    </xsd:simpleType>
    
    <xsd:complexType name="segnalazione_avvio_ctype">
        <xsd:sequence>
            <xsd:element name="esercizio_vicinato" type="esegavv:esercizio_vicinato_stype" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="esclusiva_apparecchi_automatici" type="esegavv:esclusiva_apparecchi_automatici_stype" minOccurs="0" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>
