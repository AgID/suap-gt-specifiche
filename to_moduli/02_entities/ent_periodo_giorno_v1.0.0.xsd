            <xsd:element name="da" type="epergio:ggmmaaaa_stype" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="a" type="epergio:ggmmaaaa_stype" minOccurs="1" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
